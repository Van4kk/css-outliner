{
	"manifest_version": 3,
	"name": "CSS Outliner",
	"short_name": "CSS Outliner",
	"message": "CSS Outliner",
	"description": "Adding outline for chosen HTML element.",
	"version": "1.0",
	"version_name": "1.0 Open Beta",
	"homepage_url": "https://github.com/Van4kk/CSS-Outliner",
	"author": "Cucu Ionu»õ - Nicu",
	"permissions": [
    	"activeTab"
    ],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["src/js/css-outliner.js"],
			"run_at": "document_end"
		}
	],
	"host_permissions": ["<all_urls>"],
	"icons": {
		"16": "public/logo/css-outliner-logo-default_16x16.png",
		"48": "public/logo/css-outliner-logo-default_48x48.png",
		"128": "public/logo/css-outliner-logo-default_128x128.png"
	},
	"browser_action": {
		"default_popup": "public/popup.html"
	},
	"offline_enabled": true,
	"incognito": "not_allowed"
}